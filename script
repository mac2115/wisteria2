getgenv().farm = true
getgenv().farm2 = true
getgenv().tp = true
getgenv().Moneyfarm = true
getgenv().godmode = true
getgenv().prisonofdemons = true
getgenv().wipe = true
getgenv().agilityfarm =true
getgenv().agilitytp1 = true
getgenv().instantnichirin = true
getgenv().instantslayer = true
getgenv().instantdemon = true
getgenv().thunderbreathing = true
getgenv().demonreroll = true


function rerolldemon()
spawn(function()
if getgenv().demonreroll == true then 

local args = {
    [1] = {
        ["Type"] = "End",
        ["Npc"] = workspace.Npcs:FindFirstChild("Machigai Tenshi"),
        ["Path"] = "DPath7"
    }
}

game:GetService("ReplicatedStorage").Events.Dialogue:FireServer(unpack(args))

end
end)
end

function thunder()
spawn (function()
if getgenv().thunderbreathing == trie then
    

local args = {
    [1] = {
        ["Type"] = "End",
        ["Npc"] = workspace.Npcs.Daichi,
        ["Path"] = "RepTab"
    }
}

game:GetService("ReplicatedStorage").Events.Dialogue:FireServer(unpack(args))
    
end
end)
end


function demoninstant()
spawn(function()
if getgenv().instantdemon == true then

local args = {
    [1] = {
        ["Type"] = "End",
        ["Npc"] = workspace.Npcs:FindFirstChild("Machigai Tenshi"),
        ["Path"] = "DPath7"
    }
}

game:GetService("ReplicatedStorage").Events.Dialogue:FireServer(unpack(args))

end
end)
end



function finalselectiontp()
spawn(function()
if getgenv().instantslayer == true then
   
    
game:GetService("TeleportService"):Teleport(11754723819)



end
end)
end


function nichirn()
spawn(function()
if getgenv().instantnichirin ==true then
    
local args = {
    [1] = {
        ["Type"] = "End",
        ["Npc"] = workspace.Npcs.Akihito,
        ["Path"] = "FQuest3"
    }
}

game:GetService("ReplicatedStorage").Events.Dialogue:FireServer(unpack(args))

end
end)
end


function wipe()
if getgenv().wipe == true then
    game:GetService("ReplicatedStorage").Events.Teleport:FireServer()
end
end


function prisonofdemons()
if getgenv().prisonofdemons ==true then 

    game:GetService("TeleportService"):Teleport(12851235043)

     end
end


function strenghtp()
    spawn(function()
        while getgenv().tp do task.wait()
    
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1205, 325, -2711)

        end
        end)
    end


function agilitytp()
spawn(function()
while getgenv().agilitytp1 do task.wait()
   
    
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1451, 301, -1589)

end
end)
end


function agilityfarm()
    spawn(function()
    
    while getgenv().farm2 do task.wait()
        local args = {
            [1] = {
                ["Type"] = "End",
                ["Npc"] = workspace.Npcs.Jin,
                ["Path"] = "Agility"
            }
        }
        
        game:GetService("ReplicatedStorage").Events.Dialogue:FireServer(unpack(args))
       
    end
    end)
    end



function strenghtfarm()
spawn(function()

while getgenv().farm do task.wait()
    local ohTable1 = {
        ["Type"] = "End",
        ["Npc"] = workspace.Npcs.Jin,
        ["Path"] = "Strength"
    }

   game:GetService("ReplicatedStorage").Events.Dialogue:FireServer(ohTable1)
   
end
end)
end

function moneyexp ()
    spawn(function()
while getgenv().Moneyfarm == true do task.wait()
    game:GetService("ReplicatedStorage").Events.Dialogue:FireServer({
        ["Type"] = "End",
        ["Npc"] = workspace.Npcs.Mita,
        ["Path"] = "Accept"
    })
game:GetService("ReplicatedStorage").Events.Dialogue:FireServer({
        ["Type"] = "End",
        ["Npc"] = workspace.Npcs:FindFirstChild("Saru Kenshi"),
        ["Path"] = "SetSpawn"
    })
end
end)
end

function godmode()
if getgenv().godmode == true then
    game:GetService("Players").LocalPlayer.Character.Effects.Attackers:Destroy()

end
end


local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Wisteria 2 farms - Made by Mac#0014", "DarkTheme")

local Tab = Window:NewTab("Farms")
local Section = Tab:NewSection("Autofarm")

Section:NewButton("Teleport first before clicking autofarm strenght", "Teleports you to autofarm spot", function()
    strenghtp()
    
end)

Section:NewButton("Autofarm strenght", "Farms Strenght", function()
    strenghtfarm()
    
end)

Section:NewButton("Autofarm money and exp(you need 50 cash)", "Farms money and exp", function()
    moneyexp()
    
end)

Section:NewButton("Teleport first before clicking autofarm agility", "Teleports you to autofarm spot", function()
    agilitytp()
    
end)


Section:NewButton("Autofarm Agility", "Farms agility", function()
    agilityfarm()
    
end)








local Tab = Window:NewTab("Game Teleports")
local Section = Tab:NewSection("Game Teleports")
Section:NewButton("Final Selection tp", "Godmode", function()
    finalselectiontp()
    
end)

Section:NewButton("Prison of demons teleport(must be a slayer)", "Tps you to arena promotion place", function()
    prisonofdemons()

end)


Section:NewButton("Wipe tp (must be in main menu)", "Wipes your story quests", function()
    wipe()

end)




local Tab = Window:NewTab("Misc")
local Section = Tab:NewSection("Player related")

Section:NewButton("Godmode", "Godmode", function()
    godmode()
    
end)

Section:NewButton("Instant nichirin", "Gives nichirin instantly", function()
   nichirn()
    
end)


Section:NewButton("Instant become demon", "Become demon instantly", function()
    demoninstant()
     
 end)

 Section:NewButton("Reroll demon type", "Rerolls your demon type", function()
    rerolldemon()
     
 end)


 Section:NewButton("Instant thunder breathing", "Get thunder breathing quickly", function()
    thunder()
     
 end)
